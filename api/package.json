{
  "name": "basic-api-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "debug": "npm run dev",
    "dev": "nodemon ./bin/www",
    "start": "node ./bin/www",
    "lint": "eslint **/*.js",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "code:fix": "prettier --write . && eslint --fix ."
  },
  "nodemonConfig": {
    "ignore": [
      "data/*"
    ],
    "exec": "npm run lint && node"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "escape-html": "^1.0.3",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "translate-google": "^1.5.0"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.20",
    "prettier-airbnb-config": "^1.0.0"
  },
  "author": "e-Baron"
}
